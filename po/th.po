msgstr "ทำงานอยู่"

#: src/common/dcc.c:69
msgid "Failed"
msgstr "ล้มเหลว"

#: src/common/dcc.c:70
msgid "Done"
msgstr "เรียบร้อย"

#: src/common/dcc.c:71 src/fe-gtk/menu.c:929
